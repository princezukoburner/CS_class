def triangle():
    print("     /\     ")
    print("    /  \    ")
    print("   /    \   ")
    print("  +------+  ")

def box():
    print("  +------+  ")
    print("  |      |  ")
    print("  |      |  ")
    print("  +------+  ")

def box_two():
    print("  /__\/__\  ")
    print("   ^^  ^^   ")
    print("   ^^  ^^   ")
    print("   ^^  ^^   ")
    print("  {******}  ")

def rocket():
    triangle()
    box()
    box()
    box()
    box_two()   

rocket()

## Part 2 ##

def sumSeries(a, b, c):
    return (b/2) * (2 * a + (b-1) * c)

startNumber = int(input("Start number: "))
numberOfNumbers = int(input("Number of terms: "))
differenceBetween = int(input("Difference in terms: "))

sum = sumSeries(startNumber, numberOfNumbers, differenceBetween)
print(sum)


